[VQVAE]
model_name = vqvae_minimal_10_code_books
input_dim = 30
hidden_dims = 64, 32
latent_dim = 16
num_embeddings = 64
embedding_dim = 16
commitment_cost = 0.005
decay = 0.99
dropout_rate = 0.1
classification = True
num_classes = 2
classification_weight = 1.0

[DataLoader]
dataset_name = stanpony/european_credit_card_fraud_dataset
exclude_cols = original_index
normalize_cols = Time, Amount
batch_size = 32
shuffle = True
num_workers = 4
normalize = True

[Trainer]
num_epochs = 50
save_dir = saved_models/vq_vae
validate_every = 1
save_best_only = True
early_stopping = True
patience = 15
learning_rate = 0.0002
weight_decay = 0.00001

[WandB]
use_wandb = False
project = fraud-vqvae
name = vqvae-minimal
entity = alexkstern
tags = fraud, vqvae, minimal